{
  "name": "atomer-browser-app",
  "version": "0.0.1",
  "description": "skeleton of my browser app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangjianqing/atomer-browser-app.git"
  },
  "keywords": [
    "browser",
    "app",
    "skeleton"
  ],
  "author": "cz_jjq",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jiangjianqing/atomer-browser-app/issues"
  },
  "homepage": "https://github.com/jiangjianqing/atomer-browser-app#readme",
  "browser": {
    "jquery": "./vendor/jquery-3.2.1.min.js"
  },
  "browserify-shim": {
    "jquery": "global:jQuery"
  },
  "browserify": {
    "debug": true,
    "entries": [
      "./lib/entry.js"
    ],
    "require": [
      "assert",
      "events",
      "querystring",
      "stream",
      "util",
      "buffer",
      "vue",
      "q",
      "axios",
      "handlebars"
    ],
    "transform": [
      "brfs",
      "browserify-shim"
    ]
  },
  "dependencies": {
    "axios": "^0.16.1",
    "handlebars": "^4.0.8",
    "js-cookie": "^2.1.4",
    "q": "^1.5.0",
    "vue": "^2.3.3"
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^14.3.0",
    "browserify-shim": "^3.8.14",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
