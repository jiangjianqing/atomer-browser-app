{
  "name": "atomer-browser-app",
  "version": "0.0.1",
  "description": "skeleton of my browser app",
  "main": "index.js",
  "output": "./build",
  "sourcePath": "./src",
  "app": {
      "title": "browser-app",
      "version": "2017.07"
  },
  "scripts": {
    "postcss": "postcss src/**/*.css -o build/main.css -m",
    "clean:dist": "rimraf dist",
    "lint": "run-s lint:*",
    "lint:js": "eslint \"{__tests__,test,src,pages/src,pages/lib}/**/*.js\"",
    "flow": "flow",
    "type-check": "flow check",
    "build": "run-s build:lib",
    "build:lib": "run-s clean:lib compile:lib test",
    "clean:lib": "rimraf lib",
    "compile:lib": "babel test-babel -d lib ",
    "start": "npm run debug",
    "debug": "  run-s dll:debug bundle:debug serve:debug",
    "release": "  run-s dll:release bundle:release serve:release",
    "serve:debug": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js  --open",
    "serve:release": "cross-env NODE_ENV=production webpack-dev-server --config ./webpack.config.js  --open",
    "dll:debug": "cross-env NODE_ENV=development webpack --config ./webpack.dll.config.js  --progress --colors",
    "dll:release": "cross-env NODE_ENV=production webpack --config ./webpack.dll.config.js  --progress --colors",
    "bundle:debug": "cross-env NODE_ENV=development webpack --config ./webpack.config.js  --progress --hide-modules",
    "bundle:release": "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --hide-modules",
    "test": "mocha",
    "rollup": "rollup -c --globals jquery:jQuery,router:Router",
    "predeploy": "npm run build",
    "deploy": "echo deploy!!"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangjianqing/atomer-browser-app.git"
  },
  "keywords": [
    "browser",
    "app",
    "skeleton"
  ],
  "author": "cz_jjq",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jiangjianqing/atomer-browser-app/issues"
  },
  "homepage": "https://github.com/jiangjianqing/atomer-browser-app#readme",
  "browserify-shim": {
    "jquery": "global:jQuery",
    "router": "global:Router"
  },
  "aliasify": {
    "aliases": {
      "vue": "./node_modules/vue/dist/vue.common.js"
    },
    "verbose": false
  },
  "browserify": {
    "extensions": [
      ".jsx",
      ".hbs"
    ],
    "debug": true,
    "entries": [
      "./src/main.js"
    ],
    "require": [
      "assert",
      "events",
      "querystring",
      "stream",
      "util",
      "buffer",
      "vue",
      "q",
      "axios",
      "handlebars"
    ],
    "transform": [
      "babelify",
      "vueify",
      "reactify",
      "aliasify",
      "brfs",
      "browserify-shim"
    ]
  },
  "dependencies": {
    "atomer-common-lib": "latest",
    "axios": "^0.16.2",
    "debug": "latest",
    "handlebars": "^4.0.8",
    "page": "^1.7.1",
    "q": "^1.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^14.3.0",
    "browserify-shim": "^3.8.14",
    "chai": "latest",
    "colors": "^1.1.2",
    "cpy": "^5.0.0",
    "cross-env": "latest",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-config-atomer": "latest",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "latest",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-watch": "^4.3.11",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "precss": "^1.4.0",
    "reactify": "^1.1.1",
    "rimraf": "^2.6.1",
    "rollup": "latest",
    "rollup-plugin-babel": "latest",
    "rollup-plugin-commonjs": "latest",
    "rollup-plugin-css-only": "latest",
    "rollup-plugin-eslint": "latest",
    "rollup-plugin-node-builtins": "latest",
    "rollup-plugin-node-globals": "latest",
    "rollup-plugin-node-resolve": "latest",
    "rollup-plugin-replace": "latest",
    "rollup-plugin-uglify": "latest",
    "rollup-plugin-vue2": "latest",
    "string-replace-loader": "^1.2.0",
    "style-loader": "^0.18.2",
    "transform-loader": "^0.2.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
