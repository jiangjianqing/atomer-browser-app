{
  "name": "atomer-browser-app",
  "version": "0.0.1",
  "description": "skeleton of my browser app",
  "main": "index.js",
  "output": "./build",
  "scripts": {
    "dev": "npm run build && cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js  --open",
    "test": "mocha",
    "dll": "cross-env NODE_ENV=production webpack --config ./webpack.dll.config.js  --progress --colors",
    "build-release": "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --hide-modules",
    "build": "cross-env webpack --config ./webpack.config.js  --progress --hide-modules",
    "predeploy": "npm run build",
    "deploy": "echo deploy!!"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangjianqing/atomer-browser-app.git"
  },
  "keywords": [
    "browser",
    "app",
    "skeleton"
  ],
  "author": "cz_jjq",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jiangjianqing/atomer-browser-app/issues"
  },
  "homepage": "https://github.com/jiangjianqing/atomer-browser-app#readme",
  "browser": {
    "jquery": "./vendor/jquery-3.2.1.min.js"
  },
  "browserify-shim": {
    "jquery": "global:jQuery",
    "router": "global:Router"
  },
  "aliasify": {
    "aliases": {
      "vue": "./node_modules/vue/dist/vue.common.js"
    },
    "verbose": false
  },
  "browserify": {
    "debug": true,
    "entries": [
      "./src/main.js"
    ],
    "require": [
      "assert",
      "events",
      "querystring",
      "stream",
      "util",
      "buffer",
      "vue",
      "q",
      "axios",
      "handlebars"
    ],
    "transform": [
      "babelify",
      "vueify",
      "reactify",
      "aliasify",
      "brfs",
      "browserify-shim"
    ]
  },
  "dependencies": {
    "axios": "^0.16.1",
    "handlebars": "^4.0.8",
    "js-cookie": "^2.1.4",
    "page": "^1.7.1",
    "q": "^1.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^14.3.0",
    "browserify-shim": "^3.8.14",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-config-atomer": "latest",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-react": "^7.0.1",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.0",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "mocha": "^3.2.0",
    "reactify": "^1.1.1",
    "transform-loader": "^0.2.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
